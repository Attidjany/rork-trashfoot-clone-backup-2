{
  "buildCommand": "npm run build:web",
  "installCommand": "npm install --legacy-peer-deps && npm install ajv@^8.12.0 --legacy-peer-deps",
  "outputDirectory": "dist",
  "framework": null,
  "functions": {
    "api/index.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
